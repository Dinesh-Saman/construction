const express = require('express')
const router = express.Router()
const CategorySchema = require("../models/CategoryIssue")

router.get("/get-category_issue" , async(req,res)=>{
    let category_issue = CategorySchema.find({} , function(err ,category_issue ){
        if(err){
            res.json({msg:err})
        }else{
            res.json({category_issue})
        }
    })
})

